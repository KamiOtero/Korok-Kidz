--updates
function _update()
	if state=="logo" then logo_update()
	elseif state=="dialog" then dtb_update()
	elseif state=="menu" then update_menu()
    elseif state=="win" then draw_win()
    elseif state=="lose" then draw_lose()
	else game_update() end
end

function logo_update()
    state = "logo"
    if (time() - logo_animation) > 0.75 then 
        logo += 1
        logo_animation=time()
    end
    if (logo > 2) then logo = 0 end
    if (btn(4)) then game_init() end
   end

