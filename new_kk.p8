function game_update()
	state="game"
	dialog_cooldown = dialog_cooldown - 1
    move()
    map_update()
	--collide with npc1
	if hitbox_collide(player_hitbox[1],player_hitbox[2],player_hitbox[3],player_hitbox[4],   npc1[1],npc1[2],npc1[3],npc1[4] )
	then
	 	 blocked()
	end
	
	if hitbox_collide(player_hitbox[1],player_hitbox[2],player_hitbox[3],player_hitbox[4],   dialog_npc1[1],dialog_npc1[2],dialog_npc1[3],dialog_npc1[4] )
	then
	  if btn(4) and (dialog_cooldown <= 0) then 
	    state="dialog"
	    dtb_disp("good job, you figured out you gotta press 'z' to talk to me, my name is froggo and here is the name of the game: you gotta go catch me some fresh flies, man. please i'm despretly hungry!!! go to the red square to attract some delicious flies.")
        dialog_cooldown=5
        spawn_redsquare=true
	  end
	end

    if hitbox_collide(player_hitbox[1],player_hitbox[2],player_hitbox[3],player_hitbox[4],   redsquare_box[1],redsquare_box[2],redsquare_box[3],redsquare_box[4] )
	then
        state="flies"
        redsquare_box = {-1,-1,-1,-1}
        spawn_redsquare=false
        flies=true
    end

    if (flies==true) and (points <= 10) then state ="flies" end
    if hitbox_collide(player_hitbox[1],player_hitbox[2],player_hitbox[3],player_hitbox[4],   fly[1],fly[2],fly[1]+8,fly[2]+8)
    then
       points = points + 1
       fly={rnd(112)+8, rnd(112)+8}
    end
    if (points==winflies) then state="win" end
    if (points>=(winflies + 6)) then state="lose" end
	
end



